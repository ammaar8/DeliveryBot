<robot name="elevator">

    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
		<robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        </plugin>
    </gazebo>

	<gazebo>
		<plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
			<jointName>joint1</jointName>
		</plugin>
	</gazebo>


    <link name="world"/>

    <link name="z_axis_link">
 	    <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="50" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="1.0 1.0 0.01"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="0.0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="1.0 1.0 0.01"/>
            </geometry>
        </visual>
	</link>

    <gazebo reference="z_axis_link">
        <mu1>10.0</mu1>
        <mu2>10.0</mu2>
        <material>Gazebo/Red</material>
    </gazebo>

    <joint name="joint1" type="prismatic">
        <origin xyz="0 0 0.01" rpy="0.0 0.0 0.0"/>
        <parent link="world" />
        <child link="z_axis_link" />
        <axis xyz="0 0 1" />
        <limit effort="100000" lower="0.0" upper="6.0" velocity="0.1" />
	<dynamics damping="100.0" />
    </joint>


    <transmission name="tran1">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint1">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor1">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

</robot>
